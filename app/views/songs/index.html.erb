<h1>Songs</h1>

<table>
<thead>
<tr>
<th>Artist</th>
<th>Song</th>
</tr>
</thead>
<tbody>
<% @songs.each do |song| %>
    <tr>
        <th>
        <%= song.artist_name %>
        </th>
        <th>
        <%= link_to song.title, song_path(song) %>
        </th>
    </tr>
<% end %>
</tbody>
</table>